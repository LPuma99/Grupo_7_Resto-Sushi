<!DOCTYPE html>
<html lang="en">

<head>
    <link rel="shortcut icon" href="/images/Logo/Recurso 12.png" type="image/x-icon">
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <script src="https://kit.fontawesome.com/45870a9a9c.js" crossorigin="anonymous"></script>
    <title>Resto Sushi</title>
</head>

<body>
    <!-- HEADER -->
    <header class="header-container-gral">
        <div class="header-container">
            <!-- LOGO -->
            <div class="header-container-logo">
                <img src="/images/Logo/Recurso 12.png" alt="Logo" class="img-logo">
            </div>
            <!-- BUSCADOR -->
            <div class="header-container-search">
                <form autocomplete="of" class="form-search">
                    <div class="div-input">
                        <input type="text" name="search" placeholder="BUSCAR" class="input-search">
                    </div>
                </form>
            </div>

            <!-- MENU DE NAVEGACION -->
            <nav class="header-container-nav">

                <label for="check" class="nav-label">
                    <img src="/images/icons/bars.svg" alt="bar" class="btn-nav-bars">
                </label>
                <input type="checkbox" id="check" class="check-input">
                <div class="nav-menu-link">
                    <a href="#" class="nav-link">MENU</a>
                    <a href="#" class="nav-link">MENU</a>
                    <a href="#" class="nav-link">MENU</a>
                    <!-- CARRITO DE COMPRAS PHONE-->
                    <div class="header-container-shopping-phone">
                        <img src="/images/icons/carrito-de-compras.png" class="img-shopping"
                            alt=" shopping cart">
                    </div>
                </div>

            </nav>
            <!-- CARRITO DE COMPRAS DESKTOP-->
            <div class="header-container-shopping-desktop">
                <img src="/images/icons/carrito-de-compras.png" class="img-shopping"
                    alt=" shopping cart">
            </div>
            <!-- AVATAR -->
            <div class="header-container-avatar">
                <img src="/images/icons/avatarSushi.jpg" alt="avatar" class="img-avatar">
                <div class="avatar-state">

                </div>
            </div>
        </div>
    </header>
    <!-- END HEADER -->
    <!-- INICIO -->

    <main>

        <div class="slider">
            <div class="slider_container">
                <!-- Slides -->
                <ul class="slides">
                    <li id="slide1">
                        <img src="/images/imgs/plato6.jpg" alt="Oferta1">
                    </li>
                    <li id="slide2">
                        <img src="/images/imgs/sushiman.jpg" alt="Oferta2">
                    </li>
                    <li id="slide3">
                        <img src="/images/imgs/plato4.jpg" alt="Oferta3">
                    </li>
                </ul>
                <!-- Texto central -->
                <div class="text_container">
                    <div class="texto">
                        <span><a href="#" class="btn_img">Seleccioná esta oferta</a></span>
                    </div>
                </div>
                <!-- MenuSlide -->
                <div class="menu">
                    <ul class="menu_slider">
                        <li>
                            <a href="#slide1" class="btn_img"></a>
                        </li>
                        <li>
                            <a href="#slide2" class="btn_img"></a>
                        </li>
                        <li>
